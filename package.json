{
  "name": "react-spreadsheet-cell",
  "version": "1.0.0",
  "description": "Editable spreadsheet-like cell for React",
  "type": "module",
  "license": "MIT",
  "author": "TKSOHLT",
  "repository": {
    "type": "git",
    "url": "tu-repo-url"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "dev": "vite --config docs/vite.config.ts",
    "build": "npm run build:lib && npm run build:css",
    "build:lib": "vite build --config vite.lib.config.ts",
    "build:css": "tailwindcss -i ./src/input.css -o ./dist/styles.css --minify",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^25.0.3",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^7.11.0",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4"
  }
}